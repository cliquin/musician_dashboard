<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Set List Songs

    <a data-toggle="collapse" href="#set_list_set_list_songs_new_form">
      Add set list song
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @set_list.set_list_songs.each do |set_list_song| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <a href="/set_list_songs/<%= set_list_song.id %>">
          <%= set_list_song.song_id %>
        </a>

        <a href="/delete_set_list_song_from_set_list/<%= set_list_song.id %>">
          Delete
        </a>

      </li>
    <% end %>

    <div class="card-footer">
      <div class="collapse" id="set_list_set_list_songs_new_form">
        <%= render partial: "set_list_templates/set_list_set_list_songs_new_form" %>
      </div>
    </div>

  </ul>
</div>
