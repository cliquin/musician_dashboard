<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All set list songs
    </h1>

    <a href="/set_list_songs/new" class="btn btn-block btn-outline-secondary">
      Add a new set list song
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Song
        </th>

        <th>
          Set list
        </th>

        <th>
          Position
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @set_list_songs.each do |set_list_song| %>
      <tr>
        <!-- Display set_list_song.id start -->
        <td>
          <%= set_list_song.id %>
        </td>
        <!-- Display set_list_song.id end -->

        <!-- Display set_list_song.song_id start -->
        <td>
          <%= set_list_song.song.title %>
        </td>
        <!-- Display set_list_song.song_id end -->

        <!-- Display set_list_song.set_list_id start -->
        <td>
          <%= set_list_song.set_list.title %>
        </td>
        <!-- Display set_list_song.set_list_id end -->

        <!-- Display set_list_song.position start -->
        <td>
          <%= set_list_song.position %>
        </td>
        <!-- Display set_list_song.position end -->

        <!-- Display set_list_song.created_at start -->
        <td>
          <%= time_ago_in_words(set_list_song.created_at) %> ago
        </td>
        <!-- Display set_list_song.created_at end -->

        <!-- Display set_list_song.updated_at start -->
        <td>
          <%= time_ago_in_words(set_list_song.updated_at) %> ago
        </td>
        <!-- Display set_list_song.updated_at end -->

        <td>
          <a href="/set_list_songs/<%= set_list_song.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @set_list_songs, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
